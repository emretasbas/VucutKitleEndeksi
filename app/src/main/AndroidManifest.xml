<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.envy_m6.vucutkitleendeksi" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ErkekKitleEndeksi" >
            <intent-filter>
                <action android:name="android.intent.action.ERKEKKITLEENDEKSI" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ErkekIdealKilo" >
            <intent-filter>
                <action android:name="android.intent.action.ERKEKIDEALKILO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".KadinKitleEndeksi" >
            <intent-filter>
                <action android:name="android.intent.action.KADINKITLEENDEKSI" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".KadinIdealKilo" >
            <intent-filter>
                <action android:name="android.intent.action.KADINIDEALKILO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
